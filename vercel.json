
{
    "builds": [{
        "src": "Music_sharing_portal/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.6" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Music_sharing_portal /wsgi.py"
        }
    ]
}
